{
	"name": "hominio-me",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"start": "node build",
		"dev": "bun run scripts/dev-with-tunnel.js",
		"dev:simple": "bun run zero:migrate && vite dev",
		"dev:tunnel": "bun run scripts/start-localtunnel.js",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"zero:migrate": "bun run scripts/zero-migrate.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/typography": "^0.5.18",
		"@tailwindcss/vite": "^4.1.13",
		"localtunnel": "^2.0.2",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"tailwindcss": "^4.1.13",
		"vite": "^7.1.7",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.922.0",
		"@aws-sdk/s3-request-presigner": "^3.922.0",
		"@builder.io/mitosis": "^0.12.1",
		"@iconify/svelte": "^5.0.2",
		"@modelcontextprotocol/sdk": "^1.21.1",
		"@neondatabase/serverless": "^1.0.2",
		"@polar-sh/better-auth": "^1.3.0",
		"@polar-sh/sdk": "^0.41.0",
		"@polar-sh/sveltekit": "^0.5.0",
		"@rocicorp/zero": "^0.24.0",
		"better-auth": "^1.3.34",
		"html5-qrcode": "^2.3.8",
		"hume": "0.14.0",
		"kysely": "^0.28.8",
		"kysely-neon": "^2.0.2",
		"nanoid": "^5.1.6",
		"pg": "^8.16.3",
		"qr-code-styling": "^1.9.2",
		"sharp": "^0.34.4",
		"web-push": "^3.6.7",
		"zod": "3.23.8"
	},
	"trustedDependencies": [
		"@rocicorp/zero-sqlite3"
